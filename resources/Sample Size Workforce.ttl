    rdfs:label "Workforce Size 100 - 249"^^xsd:string .

<https://tundrarich.github.io/resources/workforces/workforce250Plus> a bics1:WorkforceBand ;
    rdfs:label "Workforce Size 250 +"^^xsd:string .

<https://tundrarich.github.io/resources/industries/AllIndustries> a bics1:Industry ;
    rdfs:label "All Industries"^^xsd:string ;
    bics1:ContainedWorkforce <https://tundrarich.github.io/resources/workforces/total>,
        <https://tundrarich.github.io/resources/workforces/workforce0To99>,
        <https://tundrarich.github.io/resources/workforces/workforce100To249>,
        <https://tundrarich.github.io/resources/workforces/workforce250Plus> .

