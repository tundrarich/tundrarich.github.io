bics:Sent a rdf:Property ;
     rdfs:label "Sent" ;
     rdfs:comment "The property that refers to surveys sent to an instance of an industry." ;
     rdfs:domain bics:Survey ;
     rdfs:isDefinedBy <https://tundrarich.github.io/resources/bics-ontology#> ;
     rdfs:range rdfs:Resource . 

